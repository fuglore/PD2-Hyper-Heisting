		<effect_spawner name="smoke_11" enabled="true" object="RightShoulder" effect="units/pd2_mod_zmansion/effects/smoke_trail/smoke_distorted_small" />
		<effect_spawner name="smoke_12" enabled="true" object="RightArm" effect="units/pd2_mod_zmansion/effects/smoke_trail/smoke_distorted_small" />
		<effect_spawner name="smoke_13" enabled="true" object="RightForeArm" effect="units/pd2_mod_zmansion/effects/smoke_trail/smoke_distorted_small" />
		<effect_spawner name="smoke_14" enabled="true" object="RightForeArmRoll" effect="units/pd2_mod_zmansion/effects/smoke_trail/smoke_distorted_small" />
		<effect_spawner name="smoke_15" enabled="true" object="RightHand" effect="units/pd2_mod_zmansion/effects/smoke_trail/smoke_distorted_small" />
		<effect_spawner name="smoke_trail_03" enabled="true" object="RightHand" effect="units/pd2_mod_zmansion/effects/smoke_trail/smoke_trail_small" />
		<effect_spawner name="smoke_16" enabled="true" object="LeftUpLeg" effect="units/pd2_mod_zmansion/effects/smoke_trail/smoke_distorted_small" />
		<effect_spawner name="smoke_17" enabled="true" object="LeftLeg" effect="units/pd2_mod_zmansion/effects/smoke_trail/smoke_distorted_small" />
		<effect_spawner name="smoke_18" enabled="true" object="LeftFoot" effect="units/pd2_mod_zmansion/effects/smoke_trail/smoke_distorted_small" />
		<effect_spawner name="smoke_trail_02" enabled="true" object="LeftFoot" effect="units/pd2_mod_zmansion/effects/smoke_trail/smoke_trail_small" />
		<effect_spawner name="smoke_19" enabled="true" object="LeftToeBase" effect="units/pd2_mod_zmansion/effects/smoke_trail/smoke_distorted_small" />
		<effect_spawner name="smoke_20" enabled="true" object="RightUpLeg" effect="units/pd2_mod_zmansion/effects/smoke_trail/smoke_distorted_small" />
		<effect_spawner name="smoke_21" enabled="true" object="RightLeg" effect="units/pd2_mod_zmansion/effects/smoke_trail/smoke_distorted_small" />
		<effect_spawner name="smoke_22" enabled="true" object="RightFoot" effect="units/pd2_mod_zmansion/effects/smoke_trail/smoke_distorted_small" />
		<effect_spawner name="smoke_trail_01" enabled="true" object="RightFoot" effect="units/pd2_mod_zmansion/effects/smoke_trail/smoke_trail_small" />
		<effect_spawner name="smoke_23" enabled="true" object="RightToeBase" effect="units/pd2_mod_zmansion/effects/smoke_trail/smoke_distorted_small" />

		<effect_spawner name="poff_01" enabled="false" object="Hips" effect="units/pd2_mod_zmansion/effects/smoke_trail/smoke_puff" />
		<effect_spawner name="poff_02" enabled="false" object="Spine2" effect="units/pd2_mod_zmansion/effects/smoke_trail/smoke_puff" />
		<effect_spawner name="poff_03" enabled="false" object="RightForeArm" effect="units/pd2_mod_zmansion/effects/smoke_trail/smoke_puff" />
		<effect_spawner name="poff_04" enabled="false" object="LeftForeArm" effect="units/pd2_mod_zmansion/effects/smoke_trail/smoke_puff" />
		<effect_spawner name="poff_05" enabled="false" object="RightUpLeg" effect="units/pd2_mod_zmansion/effects/smoke_trail/smoke_puff" />
		<effect_spawner name="poff_06" enabled="false" object="LeftUpLeg" effect="units/pd2_mod_zmansion/effects/smoke_trail/smoke_puff" />
	</effects>

	<graphics>
		<graphic_group name="character" enabled="true" culling_object="g_body">

			<lod_object name="lod_body">
				<object name="g_body" 		enabled="true"  max_distance="2000"  max_draw_lod="0" />
				<object name="g_body_lod1" 	enabled="true" lod="1" >
				</object>
			</lod_object>

			<lod_object name="lod_head">
				<object name="g_head" 		enabled="true"  max_distance="2000"  max_draw_lod="0" />
				<object name="g_head_lod1" 	enabled="true" lod="1" >
				</object>
			</lod_object>
	
			<object name="s_body" enabled="true" shadow_caster="true"/>

			<object name="g_il" 		enabled="false" />
			
		</graphic_group>
	</graphics>

	<lights>
		<light name="point_light" enabled="false" multiplier="reddot" far_range="25" near_range="1" falloff="4.0" type="omni|specular" />
	</lights>

</dynamic_object>